{
  "name": "Properties - listing",
  "id": "properties",
  "icon": "fad fa-house-signal",
  "category": "sweepbright",
  "fields": [
    {
      "name": "Mode",
      "id": "mode",
      "type": "select",
      "default": "filter",
      "sync": true,
      "options": {
        "filter": "Filter",
        "recent": "Recent",
        "favorites": "Favorites"
      }
    },
    {
      "name": "Filter",
      "id": "filter",
      "type": "select",
      "default": "all",
      "sync": true,
      "options": {
        "all": "Show all",
        "sale": "Only for sale",
        "let": "Only for rent",
        "projects": "Projects (for sale)",
        "sale_non_projects": "Non-projects (for sale)"
      }
    },
    {
      "name": "Max results per page",
      "id": "max_results_per_page",
      "type": "range",
      "default": 9,
      "min": 0,
      "max": 20,
      "sync": true,
      "condition": {
        "mode": [
          "filter"
        ]
      }
    },
    {
      "name": "Results per row",
      "id": "results_per_row",
      "type": "range",
      "default": 3,
      "min": 1,
      "max": 6,
      "sync": true
    },
    {
      "name": "Geolocation distance (km)",
      "id": "geo_distance",
      "type": "range",
      "default": 10,
      "min": 0,
      "max": 100,
      "sync": true,
      "condition": {
        "mode": [
          "filter"
        ]
      }
    },
    {
      "name": "Card style",
      "id": "card_style",
      "type": "select",
      "default": "",
      "sync": true,
      "options": {
        "": "Dark",
        "light": "Light"
      }
    },
    {
      "name": "Card border radius",
      "id": "card_border_radius",
      "type": "select",
      "default": "",
      "sync": true,
      "options": {
        "": "None",
        "rounded": "Small",
        "rounded-lg": "Large",
        "rounded-xl": "XL"
      }
    },
    {
      "name": "Empty state - Call to action",
      "id": "empty_button",
      "type": "select",
      "default": "false",
      "sync": true,
      "options": {
        "false": "Disable",
        "true": "Enable"
      }
    },
    {
      "name": "Button label",
      "id": "button_label",
      "type": "text",
      "default": "Contact us",
      "condition": {
        "empty_button": [
          "true"
        ]
      }
    },
    {
      "name": "Button link",
      "id": "button_link",
      "type": "page_select",
      "default": [],
      "condition": {
        "empty_button": [
          "true"
        ]
      }
    },
    {

{
  "name": "Banner",
  "id": "banner",
  "icon": "fad fa-pager",
  "category": "sweepbright",
  "navigations": [
    "navigation"
  ],
  "fields": [
    {
      "name": "Title",
      "id": "title",
      "type": "text",
      "default": "Find your next home."
    },
    {
      "name": "Image",
      "id": "image",
      "type": "text",
      "sync": true,
      "default": "https://images.unsplash.com/photo-1493663284031-b7e3aefcae8e?ixid=MXwxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHw%3D&ixlib=rb-1.2.1&auto=format&fit=crop&w=2250&q=80"
    },
    {
      "name": "Search country",
      "id": "search_country",
      "type": "select",
      "default": "be",
      "sync": true,
      "options": {
        "be": "Belgium",
        "fr": "France",
        "uk": "United Kingdom",
        "nl": "Netherlands"
      }
    },
    {
      "name": "Search placeholder",
      "id": "search_placeholder",
      "type": "text",
      "default": "Where are you looking for a home?"
    },
    {
      "name": "Search dropdown",
      "id": "search_dropdown",
      "type": "text",
      "default": "Buy or rent?"
    },
    {
      "name": "Search dropdown: Buy",
      "id": "buy",
      "type": "text",
      "default": "Buy"
    },
    {
      "name": "Search dropdown: Rent",
      "id": "rent",
      "type": "text",
      "default": "Rent"
    },
    {
      "name": "Search button",
      "id": "search_button",
      "type": "text",
      "default": "Search"
    },
    {
      "name": "Destination page",
      "id": "destination_page",
      "type": "page_select",
      "sync": true,
      "default": [
        {
          "id": 0,
          "slug": "home",
          "order": 0
        }
      ]
    }
  ]
}

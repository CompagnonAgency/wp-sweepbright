{
  "name": "Estate - agent",
  "id": "estate-agent",
  "icon": "fad fa-id-card",
  "category": "sweepbright",
  "type": "estate",
  "fields": [
    {
      "name": "Agent Information",
      "id": "agent_information",
      "type": "group",
      "subfields": [
        {
          "name": "Agent email",
          "id": "agent_email",
          "type": "text",
          "default": "john.doe@immo-site.com",
          "sync": true
        },
        {
          "name": "Additional information",
          "id": "additional_information",
          "type": "wysiwyg",
          "default": "",
          "allow_empty": true
        }
      ],
      "default": []
    },
    {
      "name": "Border radius",
      "id": "border_radius",
      "type": "select",
      "default": "",
      "sync": true,
      "options": {
        "": "None",
        "rounded": "Small",
        "rounded-lg": "Large",
        "rounded-xl": "XL",
        "rounded-full": "Full"
      }
    },
    {
      "name": "Aspect ratio",
      "id": "aspect_ratio",
      "type": "select",
      "default": "5/6",
      "sync": true,
      "options": {
        "square": "Square",
        "4/3": "Photo",
        "5/6": "Vertical (regular)",
        "2/3": "Vertical (tall)",
        "16/9": "Horizontal (widescreen)",
        "21/9": "Horizontal (cinema)"
      }
    }
  ]
}
